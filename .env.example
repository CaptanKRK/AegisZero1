# AegisZero Environment Variables
# Copy this file to .env.local and add your actual API keys

# Gemini AI API Key
GEMINI_API_KEY=your_gemini_api_key_here

# Have I Been Pwned API (optional - uses proxy if not set)
HIBP_API_KEY=your_hibp_api_key_here

# Local AI Model Server (optional)
AI_MODEL_SERVER_URL=http://localhost:8080

# Development settings
NODE_ENV=development
DEBUG=true